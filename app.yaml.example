# Google Cloud Platform App Engine Support

runtime: php
env: flex

runtime_config:
 document_root: public

skip_files:
 - .env

env_variables:
 APP_NAME: MANAPIE
 APP_ENV: local
 APP_KEY: 
 APP_DEBUG: false
 APP_LOG_LEVEL: prod
 APP_URL: http://localhost
 APP_TIMEZONE: 'Asia/Seoul'
 APP_LOCALE: 'ko'
 
 DB_CONNECTION: mysql
 DB_HOST: localhost
 DB_DATABASE: 
 DB_USERNAME: 
 DB_PASSWORD: 
 DB_SOCKET: 
 DB_PREFIX: manapie_

 BROADCAST_DRIVER: log
 CACHE_DRIVER: file
 SESSION_DRIVER: database
 QUEUE_DRIVER: sync

 MAIL_DRIVER: smtp
 MAIL_HOST: smtp.mailtrap.io
 MAIL_PORT: 2525
 MAIL_USERNAME: null
 MAIL_PASSWORD: null
 MAIL_ENCRYPTION: null

 APP_LOG: errorlog
 LOG_DELETE: true
 
beta_settings:
   cloud_sql_instances: ""
